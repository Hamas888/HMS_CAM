{
  "name": "HMS_CAM",
  "version": "1.0.0",
  "description": "Cross-platform Camera library for embedded systems.",
  "keywords": [
    "camera",
    "embedded",
    "ov2640",
    "ov3660",
    "ov5640",
    "ov7670",
    "arduino",
    "esp-idf",
    "stm32"
  ],
  "authors": [
    {
      "name": "Hamas Saeed",
      "email": "hamasaeed888@gmail.com",
      "url": "https://github.com/Hamas888",
      "maintainer": true
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Hamas888/HMS_CAM.git"
  },
  "frameworks": ["arduino", "espidf"],
  "platforms": ["espressif32"],
  "build": {
    "flags": [
      "-std=gnu++17"
    ],
    "unflags": [
      "-std=gnu++11"
    ]
  },
  "license": "MIT",
  "homepage": "https://github.com/Hamas888/HMS_CAM",
  "examples": [
    "examples"
  ],
  "export": {
    "include": [
      "src",
      "include",
      "examples",
      "CMakeLists.txt",
      "library.json",
      "LICENSE",
      "README.md"
    ],
    "exclude": [
      "test",
      ".pio",
      ".vscode"
    ]
  }
}